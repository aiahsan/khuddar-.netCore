@model KhuddarVsModals.Models.KhuddarUser
@{
    ViewBag.title = "User Details";
}
@await Html.PartialAsync("_titleBreadView", new string[] { "User Details", "Home", "User Details" })

<div class="container">
    <div class="row">
        <div class="table-responsive">
            <table class="table table-striped" >
                <tbody>
                    <tr>
                        <th>Image</th>
                     <td></td>
                    </tr>
                    <tr>
                        <th>Name</th>
                        <td>@Model.Name</td>
                    </tr>   
                    <tr>
                        <th>Email</th>
                        <td>@Model.Email</td>
                    </tr>   
                    <tr>
                        <th>Address</th>
                        <td>@Model.Address</td>
                    </tr>   
                    <tr>
                        <th>date</th>
                        <td>@Model.date.ToString("dd-MM-yyyy")</td>
                    </tr>   
                </tbody>
               
            </table>
        </div>
    </div>
</div>
